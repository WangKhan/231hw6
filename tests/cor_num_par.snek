(fun (fact n f c)
  (let
    ((i 1) (acc 1))
    (loop
      (if (> i n)
        (break acc)
        (block
          (set! acc (* acc i))
          (set! i (+ i 1))
          (set! f (- i 1))
          (set! c (* i 3))
        )
      )
    )
  )
)

